{
    "search.useIgnoreFiles": false,
    "search.exclude": {
        "**/.pio/build": true
    },
    "C_Cpp.default.includePath": [
        "${workspaceFolder}/app/**",
        "${workspaceFolder}/src/**",
        "${workspaceFolder}/sdk/bcl/inc",
        "${workspaceFolder}/sdk/bcl/stm/inc",
        "${workspaceFolder}/sdk/sys/inc",
